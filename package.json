{
  "name": "react-shapeshifter",
  "version": "1.0.1",
  "description": "React component that composes various commonly used button shapes from three lines",
  "main": "lib/index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "npm run build:transpile",
    "build:transpile": "babel src --out-dir lib",
    "clean": "rimraf lib/** dist/** docs/ghpages *.tgz examples/**/bundle.js",
    "dist": "npm run dist:dev && npm run dist:prod",
    "dist:dev": "webpack --config webpack.build.js -d",
    "dist:prod": "NODE_ENV=production webpack --config webpack.build.js -p",
    "gh-pages": "webpack -p && cpx 'examples/basic/*' docs/ghpages && rimraf docs/ghpages/*.jsx && gh-pages -d docs/ghpages && rimraf node_modules/gh-pages/.cache",
    "lint": "standard",
    "prepare": "npm run clean && npm run build",
    "start": "webpack-dev-server",
    "test": "exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ntamas/react-shapeshifter.git"
  },
  "author": "Tamas Nepusz <ntamas@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ntamas/react-shapeshifter/issues"
  },
  "homepage": "https://github.com/ntamas/react-shapeshifter#readme",
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "babel-preset-env": "^1.7.0",
    "cpx": "^1.5.0",
    "gh-pages": "^2.0.1",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "rimraf": "^2.6.2",
    "standard": "^12.0.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  },
  "peerDependencies": {
    "react": "^15.6.0 || ^16.0.0"
  }
}
